import java.util.*;
class main {
    
	

	public static void main(String[] args) {
		DICEroller diceroller= new DICEroller();
		System.out.println(diceroller.roll());

		
	}
	
}
 class DICEroller {
	Random random ;
	int num;
	
	DICEroller(){
	
	 random = new Random();
	
	}

	

int roll(){
	num=random.nextInt(6)+1;
	return num;
}
}

