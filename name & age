import java.util.*;


public class main {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("name?");
		String name=scanner.nextLine();
		System.out.println("hello "+ name);
		int age;
		age=scanner.nextInt();
		
		System.out.println("u be "+age);
	}

}
